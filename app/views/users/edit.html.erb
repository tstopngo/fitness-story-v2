<h1>Edit Profile</h1>
<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %> <br>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %> <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %> <br>
  <%= f.label :email %>
  <%= f.text_field :email %> <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Edit Profile" %>
<%end%>
